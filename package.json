{
  "name": "wich-wayz",
  "version": "1.17.0",
  "type": "module",
  "private": true,
  "description": "Find the best sandwich shops near you! Discover your next favorite sandwich with Wich Wayz.",
  "author": "Gabe Cornejo <wich.wayz.map@gmail.com>",
  "license": "AGPL-3.0-only",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "vite --port 3100",
    "dev:web": "vite --port 3100",
    "dev:vercel": "vercel dev --listen 3001",
    "dev:all": "concurrently -k -n vercel,vite \"vercel dev --listen 3001\" \"npm run dev\"",
    "prebuild": "npm run lint && npm run test",
    "build": "tsc -b && vite build",
    "start": "vite preview --port 5000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "vite build --mode production && rollup-plugin-visualizer",
    "validate": "npm run format:check && npm run lint",
    "ci": "npm run validate && npm run test && npm run build",
    "ci:quick": "npm run lint && npm run test && npm run build",
    "test-build": "npm run format && npm run lint && npm run build && npm run analyze",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md,html,yml,yaml}": "prettier --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@libsql/client": "^0.14.0",
    "@react-input/mask": "^2.0.4",
    "@stripe/stripe-js": "^5.2.0",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/node": "^5.6.4",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "blueimp-md5": "^2.19.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^13.6.1",
    "flowbite-react": "^0.10.2",
    "fuse.js": "^7.0.0",
    "idb": "^8.0.0",
    "jose": "^5.9.6",
    "mapbox-gl": "^3.10.0",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.53.2",
    "react-icons": "^5.3.0",
    "react-map-gl": "^7.1.8",
    "react-router-dom": "^6.28.0",
    "react-select": "^5.8.3",
    "recharts": "^2.15.1",
    "stripe": "^17.4.0",
    "title-case": "^4.3.2",
    "typescript": "^5.7.3",
    "yup": "^1.6.1"
  },
  "overrides": {
    "undici": ">=6.23.0",
    "fast-xml-parser": ">=5.3.6",
    "path-to-regexp": ">=6.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@swc/core": "^1.15.11",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/blueimp-md5": "^2.18.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^22.13.0",
    "@types/react": "^18.3.18",
    "@types/react-autosuggest": "^10.1.11",
    "@types/react-dom": "^19.2.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react-swc": "3.11.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^9.13.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "happy-dom": "^20.0.11",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "rollup": "^3.29.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.17",
    "typescript-eslint": "^8.10.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  }
}
